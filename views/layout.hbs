</head>

<body ng-app="app" >
<header>
    <nav class="navbar navbar-default navbar-fixed-top">
        <div class="container">
            <div class="row">
                <div class="col-sm-4">
                    <div class="nav navbar-left">
                        {{#if title}}<a class="navbar-text" href="/">{{title}}</a>{{/if}}
                    </div>
                </div>
                <div class="col-sm-8">
                    <div class="nav navbar-right">
                        <p class="navbar-text"><a class="navbar-link" ui-sref="search.create" title="Create Snippet">Add New Snippet <span class="glyphicon glyphicon-plus-sign"></span></a></p>
                        {{#if isAuthenticated}}
                            <p class="navbar-text">Signed in as
                                {{#if user.name}}
                                    {{user.name}}
                                {{else}}
                                    {{user.username}}
                                {{/if}}
                                    &nbsp;<a class="navbar-link" href="/logout" title="Logout"><span class="glyphicon glyphicon-log-out"></span></a></p>
                                {{else}}
                            <p class="navbar-text"><a class="navbar-link" href="/login" title="Login">Login <span class="glyphicon glyphicon-log-in"></span></a></p>
                        {{/if}}
                    </div>
                </div>
            </div>
        </div>
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    <ui-breadcrumbs displayname-property="data.displayName"></ui-breadcrumbs>
                </div>
            </div>
        </div>
    </nav>
</header>
<main class="container">
    <div class="row">
        <div class="col-md-12">
            <section ui-view></section>     <!-- AngularJS ui-routing takes control -->
        </div>
    </div>
</main>
<footer>
    <nav class="navbar navbar-default navbar-fixed-bottom">
        <div class="container">
            <div class="row">
                <div class="nav">
                    {{#if title}}<p class="navbar-text">&copy; {{title}} 2016</p>{{/if}}
                </div>
            </div>
        </div>
    </nav>
</footer>
</body>
</html>